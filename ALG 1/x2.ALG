Algoritmo "semnome"

Var
numero: Real


Inicio
 Escreva("Digite um número: ")
   Leia(numero)

   Se numero > 0 entao
      Escreva("O número é positivo.")
   senao
      Se numero < -1 entao
         Escreva("O número é negativo.")
      senao
           se numero = 0 entao
         Escreva("O número é nulo.")
      FimSe
   FimSe

    fimse
Fimalgoritmo